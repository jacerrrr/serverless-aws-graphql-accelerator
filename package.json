{
  "name": "serverless-aws-graphql-accelerator",
  "version": "1.0.0",
  "description": "Accelerator for developing GraphQL Apis with AWS Lambda and API Gateway using The Serverless Framework",
  "main": "handler.js",
  "scripts": {
    "start": "concurrently \"npm run start:dynamodb\" \"npm run start:app\"",
    "start:app": "export SLS_DEBUG=* && node --inspect node_modules/.bin/serverless offline start -s local",
    "start:dynamodb": "docker-compose up",
    "debug": "concurrently \"npm run start\" \"npm run test:watch\"",
    "build": "concurrently \"npm run build:layer\" \"npm run build:app\"",
    "build:app": "npm run clean && tsc --project src/tsconfig.app.json",
    "build:layer": "rimraf \".layer/nodejs/**/*\" && cp-cli ./package.json .layer/nodejs/package.json && cd .layer/nodejs && npm install --production",
    "clean": "rimraf dist",
    "lint": "eslint --ext .js,.ts --fix ./",
    "prettier": "prettier --write '{,!(node_modules)/**/}*.{ts,js,json,md,yml}'",
    "test": "ts-node -r tsconfig-paths/register -P src/tsconfig.spec.json node_modules/.bin/jasmine",
    "test:watch": "nodemon --ext spec.ts --exec npm test",
    "test:coverage": "ts-node  -r tsconfig-paths/register -P src/tsconfig.spec.json node_modules/.bin/nyc jasmine"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacerrrr/serverless-aws-graphql-accelerator.git"
  },
  "keywords": [
    "serverless",
    "aws",
    "graphql",
    "gateway",
    "lambda",
    "typescript",
    "webpack",
    "accelerator"
  ],
  "author": "Jace Allison",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacerrrr/serverless-aws-graphql-accelerator/issues"
  },
  "homepage": "https://github.com/jacerrrr/serverless-aws-graphql-accelerator#readme",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/aws-lambda": "^8.10.44",
    "@types/jasmine": "^3.5.6",
    "@types/node": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "aws-sdk": "^2.626.0",
    "concurrently": "^5.1.0",
    "cp-cli": "^2.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^4.2.5",
    "iterall": "^1.3.0",
    "jasmine": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.2",
    "moment": "^2.26.0",
    "moment-locales-webpack-plugin": "^1.2.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "serverless": "^1.74.1",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-offline": "^5.12.1",
    "serverless-webpack": "^5.3.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.8.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "apollo-server-core": "^2.15.1",
    "apollo-server-lambda": "^2.15.1",
    "class-validator": "^0.12.2",
    "graphql": "^15.3.0",
    "reflect-metadata": "^0.1.13",
    "subscriptions-transport-ws": "^0.9.17",
    "type-graphql": "^1.0.0-rc.3",
    "typedi": "^0.8.0"
  }
}
